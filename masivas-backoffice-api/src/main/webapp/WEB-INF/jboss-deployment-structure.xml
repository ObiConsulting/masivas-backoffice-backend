<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.3">
  <deployment>
    <exclude-subsystems>
      <subsystem name="logging" />
    </exclude-subsystems>
    <exclusions>
      <module name="org.apache.logging.log4j" />
      <module name="org.springframework.spring" />
    </exclusions>
  </deployment>
</jboss-deployment-structure>